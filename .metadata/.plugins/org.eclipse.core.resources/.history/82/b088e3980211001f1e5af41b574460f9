package myCodes;

			/** Method Overriding:
			 * 	RunTime Polymorphism or Static Binding
			 * 	can achieve by Inheritance concept
			 * 
			 * 	Rules:
			 * 	Both Parent Class and Child Class method name should be same
			 * 	Static and Final Methods cannot be Overridden
			 * 
			 * 	child method will execute first as we are creating object for child.
			 */

public class MethodOverridng_concept extends MethodOverriding_Parent{
	public void parentRP() {
		System.out.println("child RP");
	}
			
	public static void main(String[] args) {
		MethodOverridng_concept mc = new MethodOverridng_concept();
		mc.parentRP();
		mc.ParentRP();
	}

}
